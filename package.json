{
  "name": "maas",
  "version": "0.0.1",
  "description": "MongoDB as a Service",
  "main": "./dist/main.js",
  "bin": "./dist/maas.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "burstmake": "rimraf ./**/*.*~ && rimraf ./**/tsconfig.json && burstmake",
    "typedoc": "typedoc",
    "doc": "npm run typedoc -- --options typedoc.json --out ./doc/source ./src/ && apidoc -i src/ -o doc/api/",
    "clean": "rimraf ./dist/*",
    "build": "tsc -p ./src/ && tsc -p ./src/public/",
    "rebuild": "npm run clean && npm run build",
    "dev-launch": "npm run rebuild && npm test && npm start",
    "pretest": "tsc -p ./test/",
    "test": "rimraf ./{src,test}/**/*.d.ts && mocha ./test/**/*.js",
    "lint": "tslint ./{src,test}/**/*.ts",
    "typings": "typings install && cd  ./src/public && typings install && cd -",
    "start": "node ./dist/maas.js",
    "postinstall": "npm run burstmake && npm run typings"
  },
  "engines": {
    "node": "4.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BugBusterSWE/MaaS"
  },
  "bugs": {
    "url": "https://github.com/BugBusterSWE/MaaS/issues"
  },
  "keywords": [
    "api",
    "service",
    "mongodb"
  ],
  "author": "BugBusters bugbusterswe@gmail.com",
  "license": "MIT",
  "dependencies": {
    "express": "^4.13.1",
    "mongoose": "^4.4.14",
    "promise": "^7.1.1",
    "body-parser": "^1.15.1",
    "helmet": "^2.0.0",
    "flux": "^2.1.1",
    "history": "^2.1.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0",
    "webpack-dev-server": "^1.14.1",
    "superagent": "1.8.1"
  },
  "devDependencies": {
    "apidoc": "^0.15.1",
    "burstmake": "latest",
    "chai": "^3.5.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "tslint": "^3.5.0",
    "typedoc": "latest",
    "typescript": "^1.8.7",
    "typings": "^0.8.1",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "webpack": "^1.12.3",
    "webpack-typescript": "^0.5.6"
  }
}
